{
  "name": "collections",
  "version": "0.0.1",
  "description": "Data structures in JavaScript",
  "main": "collections.js",
  "types": "collections.d.ts",
  "engines": {
    "node": "16 || 18 || 20 || 22"
  },
  "readmeFilename": "README.md",
  "scripts": {
    "test": "npm run lint && npm run types && tap run",
    "types": "tsc -p tsconfig.json",
    "lint": "eslint . && prettier --check \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\""
  },
  "keywords": [
    "data structures",
    "collections",
    "queue",
    "deque",
    "stack",
    "heap",
    "hash table",
    "linked list",
    "graph",
    "tree"
  ],
  "author": "Timur Sevimli <svmlitimur@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timursevimli/collections/issues",
    "email": "svmlitimur+github@gmail.com"
  },
  "homepage": "https://github.com/timursevimli/collections#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "tap": "^21.0.0",
    "typescript": "^5.5.3"
  }
}
